{
  "manifest_version": 2,

  "name": "Airomo: Got Apps?",
  "version": "0.1.2",
  "description": "Get contextually relevant mobile apps.",
  "icons": { "128": "icon_128.png" },
  "background": {
    "persistent": false,
    "scripts": ["bg.js"]
  },
  "permissions": ["tabs", "http://static.airomo.com/"],
  "browser_action": {
    "default_title": "Get contexually relevant mobile apps.",
    "default_icon": "icon_19.png"
  }
}